#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules/sane/src/cli.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules/sane/src/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules/sane/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../sane/1.6.0/node_modules/sane/src/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules/sane/src/cli.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules/sane/src/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules/sane/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/1.6.0/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sane/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../sane/1.6.0/node_modules/sane/src/cli.js" "$@"
  ret=$?
fi
exit $ret

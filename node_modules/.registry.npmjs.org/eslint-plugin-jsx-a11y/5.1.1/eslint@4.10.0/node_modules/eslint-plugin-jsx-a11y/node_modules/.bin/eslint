#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules/eslint/bin/eslint.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules/eslint/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules/eslint/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../../eslint/4.10.0/node_modules/eslint/bin/eslint.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules/eslint/bin/eslint.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules/eslint/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules/eslint/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/4.10.0/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/eslint/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../../eslint/4.10.0/node_modules/eslint/bin/eslint.js" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules/handlebars/bin/handlebars/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules/handlebars/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules/handlebars/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../handlebars/4.0.11/node_modules/handlebars/bin/handlebars" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules/handlebars/bin/handlebars/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules/handlebars/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules/handlebars/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/4.0.11/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/handlebars/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../handlebars/4.0.11/node_modules/handlebars/bin/handlebars" "$@"
  ret=$?
fi
exit $ret

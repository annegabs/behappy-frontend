#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/1.2.0/node_modules/is-ci/bin.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/1.2.0/node_modules/is-ci/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/1.2.0/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../is-ci/1.2.0/node_modules/is-ci/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/1.2.0/node_modules/is-ci/bin.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/1.2.0/node_modules/is-ci/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/1.2.0/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/is-ci/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../is-ci/1.2.0/node_modules/is-ci/bin.js" "$@"
  ret=$?
fi
exit $ret

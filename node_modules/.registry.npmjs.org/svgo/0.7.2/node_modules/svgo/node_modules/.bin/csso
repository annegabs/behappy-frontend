#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules/csso/bin/csso/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules/csso/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules/csso/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../csso/2.3.2/node_modules/csso/bin/csso" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules/csso/bin/csso/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules/csso/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules/csso/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/2.3.2/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/csso/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../csso/2.3.2/node_modules/csso/bin/csso" "$@"
  ret=$?
fi
exit $ret

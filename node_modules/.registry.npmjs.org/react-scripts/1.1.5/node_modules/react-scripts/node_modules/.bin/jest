#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules/jest/bin/jest.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules/jest/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules/jest/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../jest/20.0.4/node_modules/jest/bin/jest.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules/jest/bin/jest.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules/jest/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules/jest/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/20.0.4/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/jest/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../jest/20.0.4/node_modules/jest/bin/jest.js" "$@"
  ret=$?
fi
exit $ret

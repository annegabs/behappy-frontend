#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules/webpack/bin/webpack.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules/webpack/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules/webpack/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../webpack/3.8.1/webpack@3.8.1/node_modules/webpack/bin/webpack.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules/webpack/bin/webpack.js/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules/webpack/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules/webpack/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/webpack@3.8.1/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/3.8.1/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/webpack/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../webpack/3.8.1/webpack@3.8.1/node_modules/webpack/bin/webpack.js" "$@"
  ret=$?
fi
exit $ret

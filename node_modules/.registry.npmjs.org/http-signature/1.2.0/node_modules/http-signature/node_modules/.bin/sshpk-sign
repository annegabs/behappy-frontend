#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules/sshpk/bin/sshpk-sign/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules/sshpk/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules/sshpk/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../sshpk/1.14.2/node_modules/sshpk/bin/sshpk-sign" "$@"
  ret=$?
else 
  NODE_PATH="/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules/sshpk/bin/sshpk-sign/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules/sshpk/bin/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules/sshpk/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/1.14.2/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/sshpk/node_modules:/home/aluno/behappy-frontend/node_modules/.registry.npmjs.org/node_modules:/home/aluno/behappy-frontend/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../sshpk/1.14.2/node_modules/sshpk/bin/sshpk-sign" "$@"
  ret=$?
fi
exit $ret
